{
    "name": "ygo.tools",
    "version": "0.0.1",
    "repository": "https://github.com/AntiTcb/ygo.tools",
    "private": true,
    "scripts": {
        "dev": "vite dev",
        "build": "vite build",
        "preview": "vite preview",
        "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
        "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
        "lint": "eslint . --max-warnings=0",
        "lint:fix": "eslint . --fix",
        "format": "prettier --write --plugin prettier-plugin-svelte --plugin prettier-plugin-tailwindcss .",
        "format:check": "prettier --check --plugin prettier-plugin-svelte --plugin prettier-plugin-tailwindcss .",
        "test": "vitest run",
        "test:watch": "vitest",
        "test:unit": "vitest run src",
        "test:e2e": "playwright test"
    },
    "devDependencies": {
        "@iconify-json/clarity": "^1.1.13",
        "@iconify-json/fa-solid": "^1.1.8",
        "@iconify-json/fa6-regular": "^1.1.21",
        "@iconify-json/heroicons": "^1.1.23",
        "@iconify-json/lucide": "^1.1.204",
        "@iconify-json/mdi": "^1.1.67",
        "@iconify-json/vscode-icons": "^1.1.36",
        "@skeletonlabs/skeleton": "3.0.0-next.2",
        "@skeletonlabs/skeleton-svelte": "1.0.0-next.4",
        "@sveltejs/adapter-auto": "^3.2.2",
        "@sveltejs/enhanced-img": "^0.3.1",
        "@sveltejs/kit": "^2.5.19",
        "@sveltejs/vite-plugin-svelte": "^3.1.1",
        "@tailwindcss/forms": "^0.5.7",
        "@tailwindcss/typography": "^0.5.13",
        "@types/eslint": "^8.56.11",
        "@types/node": "^20.14.13",
        "@types/nprogress": "^0.2.3",
        "autoprefixer": "^10.4.19",
        "eslint": "^9.8.0",
        "eslint-config-prettier": "^9.1.0",
        "eslint-plugin-svelte": "^2.43.0",
        "globals": "^15.9.0",
        "nprogress": "^0.2.0",
        "postcss": "^8.4.40",
        "postcss-import": "^16.1.0",
        "prettier": "^3.3.3",
        "prettier-plugin-svelte": "^3.2.6",
        "prettier-plugin-tailwindcss": "^0.6.5",
        "sk-seo": "^0.4.1",
        "super-sitemap": "^0.15.0",
        "svelte": "5.0.0-next.205",
        "svelte-check": "^3.8.5",
        "tailwindcss": "^3.4.7",
        "tslib": "^2.6.3",
        "typescript": "^5.5.4",
        "typescript-eslint": "^8.0.0",
        "unplugin-icons": "^0.19.1",
        "vite": "^5.3.5",
        "vite-plugin-mkcert": "^1.17.5",
        "vite-plugin-tailwind-purgecss": "^0.3.3",
        "vitest": "^1.6.0"
    },
    "type": "module"
}
